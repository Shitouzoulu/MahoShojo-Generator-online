# 数据库配置
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your_password_here
DB_NAME=mahoshojo
DB_PORT=3306

# AI 提供商配置
AI_PROVIDERS_CONFIG='[
  {
    "name": "gemini_provider", 
    "apiKey": "your_gemini_api_key_here",
    "baseUrl": "https://xxx.com/v1",
    "model": "gemini-2.5-flash"
  },
  {
    "name": "gemini_provider", 
    "apiKey": "your_gemini_api_key_here",
    "baseUrl": "https://generativelanguage.googleapis.com/v1beta",
    "model": "gemini-2.5-flash"
  }
]'

# 应用配置
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:3000

# JWT 配置
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# 文件上传配置
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# Redis 配置（可选，用于缓存和会话）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# 队列系统配置
# 每分钟最大API请求数（控制向AI服务器的请求频率）
QUEUE_MAX_REQUESTS_PER_MINUTE=10
# 用户等待时间（秒，同一用户两次请求之间的最小间隔时间）
QUEUE_USER_WAIT_TIME_SECONDS=30

# AI负载均衡配置
# 负载均衡策略: sequential(顺序) | random(随机) | round_robin(轮询)
AI_LOAD_BALANCE_STRATEGY=random

# Cloudflare D1 数据库配置
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
D1_DATABASE_ID=your_d1_database_id_here

NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX