{
    "name": "mahoshojo-generator",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "dev:pretty": "next dev --turbopack | pino-pretty",
        "dev:server": "ts-node server/index.ts",
    "test:server": "ts-node server/test-server.ts",
    "simple:server": "ts-node server/simple-test-server.ts",
        "dev:server:watch": "ts-node --watch server/index.ts",
        "server": "node scripts/start-server.js",
        "server:dev": "node scripts/start-server.js dev",
        "server:watch": "node scripts/start-server.js watch",
        "server:prod": "node scripts/start-server.js prod",
        "build": "next build",
        "build:server": "tsc --project server/tsconfig.json",
        "start": "next start",
        "start:server": "node dist/server/index.js",
        "lint": "next lint",
        "lint:server": "eslint server/**/*.ts",
        "test": "bun test",
        "test:watch": "bun test --watch",
        "test:database": "ts-node scripts/test-database.ts",
        "test:db-simple": "node scripts/simple-test.js",
            "deploy:aliyun": "bash scripts/deploy-aliyun.sh",
    "deploy:aliyun:simple": "bash scripts/deploy-aliyun-simple.sh",
    "deploy:aliyun:simple:ps": "powershell -ExecutionPolicy Bypass -File scripts/deploy-aliyun-simple.ps1",
    "deploy:workbench": "bash scripts/deploy-workbench.sh",
    "deploy:git": "bash scripts/deploy-git.sh",
    "deploy:build": "npm run build && npm run build:server",
        "docker:build": "docker build -t mahoshojo-generator .",
        "docker:run": "docker run -p 3000:3000 mahoshojo-generator",
        "health:check": "curl -f http://localhost:3000/health || exit 1"
    },
    "dependencies": {
        "@ai-sdk/google": "^1.2.22",
        "@ai-sdk/openai": "^1.3.22",
        "@next/third-parties": "^15.4.5",
        "@zumer/snapdom": "^1.9.7",
        "ai": "^4.3.16",
        "bcryptjs": "^2.4.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.1",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.511.0",
        "morgan": "^1.10.1",
        "mysql2": "^3.14.3",
        "next": "15.3.2",
        "pino": "^9.7.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "socket.io": "^4.8.1",
        "string-random": "^0.1.3",
        "tailwind-merge": "^3.3.0",
        "zod": "^3.25.67"
    },
    "devDependencies": {
        "@cloudflare/next-on-pages": "^1.13.13",
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.23",
        "@types/morgan": "^1.9.10",
        "@types/mysql": "^2.15.27",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.3.2",
        "tailwindcss": "^4",
        "ts-node": "^10.9.2",
        "tw-animate-css": "^1.3.0",
        "typescript": "^5",
        "wrangler": "^4.28.0"
    }
